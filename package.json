{
  "name": "obsidian-plaintext",
  "version": "0.3.0",
  "description": "Open any file as if it was plaintext directly in Obsidian.",
  "main": "main.js",
  "scripts": {
    "dev": "yarn node esbuild.config.mjs",
    "build": "yarn tsc -noEmit -skipLibCheck && yarn node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "yarn eslint --debug --fix src/*.ts"
  },
  "keywords": [],
  "author": "dbarenholz",
  "license": "MIT",
  "dependencies": {
    "@codemirror/autocomplete": "^6.9.0",
    "@codemirror/commands": "^6.2.5",
    "@codemirror/lang-python": "^6.1.3",
    "@codemirror/language": "^6.9.0",
    "@codemirror/lint": "^6.4.1",
    "@codemirror/search": "^6.5.2",
    "@codemirror/state": "^6.2.1",
    "@codemirror/view": "^6.16.0",
    "@lezer/highlight": "^1.1.6",
    "@types/node": "^20.5.6",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "builtin-modules": "3.3.0",
    "codemirror": "^6.0.1",
    "esbuild": "0.19.2",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "obsidian": "latest",
    "prettier": "^3.0.2",
    "prettier-eslint": "^15.0.1",
    "tslib": "2.6.2",
    "typescript": "<5.2.0",
    "typescript-eslint": "^0.0.1-alpha.0"
  },
  "packageManager": "yarn@3.6.3"
}
